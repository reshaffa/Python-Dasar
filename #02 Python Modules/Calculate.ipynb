    "# Create a sample modules\n",
    "def penjumlahan(x=0,y=0):\n",
    "    return x+y\n",
    "def pengurangan(x=0,y=0):\n",
    "    return x-y\n",
    "def perkalian(x=0,y=0):\n",
    "    return x*y\n",
    "def pembagian(x=0,y=0):\n",
    "    return x/y\n",
    "def modulus(x=0,y=0):\n",
    "    return x%y"
